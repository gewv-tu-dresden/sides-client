{
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.3.10"
  },
  "scripts": {
    "build": "yarn run openapi-generator-cli generate -i https://tek-ekg.iet.mw.tu-dresden.de/api/v1/api-docs -g python -o . --additional-properties=packageVersion=1.0.2,projectName=gewv-sides-client,packageName=gewv_sides_client",
    "bundle": "python3 -m build",
    "push": "python3 -m twine upload dist/*"
  }
}